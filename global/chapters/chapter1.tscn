				$\"../level9\".visible = true
				$\"../left\".disabled = false
				$\"../right\".disabled = false
				$\"../back\".disabled = false
				$\"../crowns\".texture = preload(\"res://resources/Game buttons/0_crowns.png\")
				$\"../crowns\".visible = true
				current+=1
			else:
				$\"../notif\".visible = true
				$\"../notif\".text = \"level is not completed\"
			
			
